// metodo com o mesmo nome da classe(OK)
class m
{
	
   public static void main(String[] args)
   {
      System.out.println(new a().i());
   }
}

class b
{
   a a;
   int b;
   public a a(){a = new a(); return this;}
   public int i(){ return 0; }
}

class a extends b
{
   a a;
   a b;
   public a a(){a = new a(); return this;}
   public int i(){ return 0; }
}
